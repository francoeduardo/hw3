
createCanvas(340,340);

background (240);
strokeWeight(20);


for (var x=10; x<=170; x=x+40) {
//upperleft
  line(x,1,x,170);

//uperright
  line(170,x,340,x);
}


for (var y=10; y<=170; y=y+55) {
//lowerleft
  line(1,170+y,y,170);
  line(y,340,170,170+y);
  
 //lowerright
  line(170,170+y,340-y,340);
  line(210+y,170,340,300-y);
}

//squaresOutline
noFill();
rect(10,10,160,160);
rect(170,10,160,160);
rect(170,170,160,160);
rect(10,170,160,160);



