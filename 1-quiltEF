

function efPatch(x, y) {
  noFill();
  noStroke();
  rect(x, y, 100, 100);
  
  // your code here!
  fill(190,40,30);
  rect(x,y,100,100);
  
  fill(0);
  rect(x,y,100,20);
  rect(x,y+40,100,20);
  rect(x,y+80,100,20);
  
  fill(255);
  rect(x+28,y+15,50,15);
  rect(x+28,y+50,40,15);
  rect(x+28,y+20,15,70);
  
  
}

//background(255);
//efPatch(0, 0);

background(255);
efPatch(width-100, height-100);
